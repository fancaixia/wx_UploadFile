.page-section-title{
  padding:20rpx 32rpx;
  font-size:32rpx;

}
.btn{
  width: calc(100% - 80rpx);
  height: 80rpx;
  margin-top: 80rpx;
}
.imgsBox{
    background-color: #fff;
    box-sizing: border-box;
    font-size: 32rpx;
    color: #333;
    display: flex;
    align-items: center;
    padding:0 30rpx;
    flex-wrap:wrap;
}
.imgsBox .optName{
    width: 200rpx;
}
.imgsBox .optName text,.imgsBox .mesTit text{
    color: #ce0a0a;
}
.imgsBox .optContent{
    flex: 1;
    text-align: right;
    color: #666;
    font-size: 32rpx;
    font-size:24rpx;
    color:#999;
}

.imgsBox .uploadimgbox{
    flex: 0 0 33.333%;
    position: relative;
    box-sizing: border-box;
    padding: 10rpx;
    margin-bottom: 10rpx;
}
.imgsBox .uploadimgbox .progress{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, .5);
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    line-height: 208rpx;
    z-index: 99;
}
.imgsBox .uploadimgbox .delbtn{
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    top: 10rpx;
    right: 10rpx;
    background-color: rgba(0, 0, 0, .5);
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    line-height: 50rpx;
    z-index: 98;
}
.imgsBox .uploadimg,.imgsBox .uploadbtn{
    display: block;
    width: 100%;
    height: 208rpx;
}
